---
layout: none
---
[{% for facet in site.data.maps-facets-daily.map-facets-TODAY.facets %}
  {% if facet.type == "original-format"%}
  {% for filter in facet.filters %}
  {
    "count": "{{ filter.count }}",
    "facet": "{{ filter.title }}"
  }{% if forloop.last == false %},{% endif %}
  {% endfor %}
  {% break %}
  {% endif %}
{% endfor %}]